{
  "name": "katalog",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "description": "Create professional PDF catalogs from any e-commerce site",
  "author": "Katalog Team",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "electron:dev": "NODE_ENV=development electron .",
    "electron:build": "next build && electron-builder --mac",
    "electron:build:win": "next build && electron-builder --win",
    "electron:build:all": "next build && electron-builder --mac --win"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@imgly/background-removal": "^1.7.0",
    "@mendable/firecrawl-js": "^4.6.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@sparticuz/chromium": "^141.0.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.85.0",
    "ag-psd": "^29.0.0",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "jsdom": "^27.4.0",
    "jspdf": "^4.0.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.555.0",
    "next": "^16.1.3",
    "openai": "^6.16.0",
    "puppeteer": "^24.31.0",
    "puppeteer-core": "^24.31.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.4",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "build": {
    "appId": "com.katalog.app",
    "productName": "Katalog",
    "copyright": "Copyright Â© 2024 Katalog",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      ".next/**/*",
      "electron/**/*",
      "public/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "public/icon.icns",
      "identity": null,
      "hardenedRuntime": false
    },
    "dmg": {
      "title": "Katalog",
      "backgroundColor": "#ffffff"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "publish": {
      "provider": "github",
      "owner": "your-github-username",
      "repo": "katalog"
    }
  }
}